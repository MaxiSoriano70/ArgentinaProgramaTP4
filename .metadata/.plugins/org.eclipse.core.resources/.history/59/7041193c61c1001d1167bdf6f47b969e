	public static void ordenar(int[] array, String m) {
		if (m == "A" || m == "a") {
			int temporal = 0;
			for (int i = 0; i < m.length; i++) {
				for (int j = 1; j < (m.length - i); j++) {
					if (m[j - 1] > m[j]) {
						temporal = m[j - 1];
						m[j - 1] = m[j];
						m[j] = temporal;
					}
				}
			}
			System.out.println(Arrays.toString(m));
		} else if (m == "B" || m == "b") {

		} else {

		}
	}

}
